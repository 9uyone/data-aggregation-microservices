{
	"Routes": [
		// Health checks
		{
			"DownstreamPathTemplate": "/health",
			"DownstreamScheme": "http",
			"DownstreamHostAndPorts": [
				{
					"Host": "auth",
					"Port": 8080
				}
			],
			"UpstreamPathTemplate": "/health/auth",
			"UpstreamHttpMethod": [ "GET" ]
		},
		{
			"DownstreamPathTemplate": "/health",
			"DownstreamScheme": "http",
			"DownstreamHostAndPorts": [
				{
					"Host": "collector",
					"Port": 8080
				}
			],
			"UpstreamPathTemplate": "/health/collector",
			"UpstreamHttpMethod": [ "GET" ]
		},
		{
			"DownstreamPathTemplate": "/health",
			"DownstreamScheme": "http",
			"DownstreamHostAndPorts": [
				{
					"Host": "scheduler",
					"Port": 8080
				}
			],
			"UpstreamPathTemplate": "/health/scheduler",
			"UpstreamHttpMethod": [ "GET" ]
		},
		{
			"DownstreamPathTemplate": "/health",
			"DownstreamScheme": "http",
			"DownstreamHostAndPorts": [
				{
					"Host": "storage",
					"Port": 8080
				}
			],
			"UpstreamPathTemplate": "/health/storage",
			"UpstreamHttpMethod": [ "GET" ]
		},

		{
			"DownstreamPathTemplate": "/collector/{everything}",
			"DownstreamScheme": "http",
			"DownstreamHostAndPorts": [
				{
					"Host": "collector",
					"Port": 8080
				}
			],
			"UpstreamPathTemplate": "/api/collector/{everything}",
			"UpstreamHttpMethod": [ "GET", "POST" ]
		},
		{
			"DownstreamPathTemplate": "/storage/{everything}",
			"DownstreamScheme": "http",
			"DownstreamHostAndPorts": [
				{
					"Host": "storage",
					"Port": 8080
				}
			],
			"UpstreamPathTemplate": "/api/storage/{everything}",
			"UpstreamHttpMethod": [ "GET", "POST", "PATCH", "DELETE" ],
			"AuthenticationOptions": {
				"AuthenticationProviderKey": "Bearer",
				"AllowedScopes": []
			}
		},
		{
			"DownstreamPathTemplate": "/auth/{everything}",
			"DownstreamScheme": "http",
			"DownstreamHostAndPorts": [
				{
					"Host": "auth",
					"Port": 8080
				}
			],
			"UpstreamPathTemplate": "/api/auth/{everything}",
			"UpstreamHttpMethod": [ "POST", "GET" ]
		}
	],
	"GlobalConfiguration": {
		"BaseUrl": "https://localhost:8081"
	}
}