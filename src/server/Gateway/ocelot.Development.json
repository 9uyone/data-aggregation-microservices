{
	"Routes": [
		// Health checks
		{
			"DownstreamHostAndPorts": [
				{
					"Host": "localhost",
					"Port": 5065
				}
			],
			"UpstreamPathTemplate": "/health/auth"
		},
		{
			"DownstreamHostAndPorts": [
				{
					"Host": "localhost",
					"Port": 5139
				}
			],
			"UpstreamPathTemplate": "/health/collector"
		},
		{
			"DownstreamHostAndPorts": [
				{
					"Host": "localhost",
					"Port": 5133
				}
			],
			"UpstreamPathTemplate": "/health/scheduler"
		},
		{
			"DownstreamHostAndPorts": [
				{
					"Host": "localhost",
					"Port": 5044
				}
			],
			"UpstreamPathTemplate": "/health/storage"
		},

		// Other routes
		{
			"DownstreamPathTemplate": "/auth/{everything}",
			"DownstreamHostAndPorts": [
				{
					"Host": "localhost",
					"Port": 5065
				}
			],
			"UpstreamPathTemplate": "/api/auth/{everything}"
		},
		{
			"DownstreamHostAndPorts": [
				{
					"Host": "localhost",
					"Port": 5139
				}
			],
			"UpstreamPathTemplate": "/api/collector/{everything}"
		},
		{
			"DownstreamHostAndPorts": [
				{
					"Host": "localhost",
					"Port": 5044
				}
			],
			"UpstreamPathTemplate": "/api/storage/{everything}"
		},

		// Local only
		{
			"DownstreamPathTemplate": "/hangfire/{everything}",
			"DownstreamScheme": "http",
			"DownstreamHostAndPorts": [
				{
					"Host": "localhost",
					"Port": 5133
				}
			],
			"UpstreamPathTemplate": "/hangfire/{everything}",
			"UpstreamHttpMethod": [ "POST", "GET", "PUT", "DELETE" ]
		}
	],
	"GlobalConfiguration": {
		"BaseUrl": "https://localhost:5001"
	}
}